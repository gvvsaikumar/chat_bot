{
    "version":2,
    "builds":[
        {
            "src":"./index.ts",
            "use":"@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/public/(.*)",  
            "dest": "/public/$1"    
        },
        {
            "src":"/(.*)",
            "dest":"./index.ts"
        }
    ]
}